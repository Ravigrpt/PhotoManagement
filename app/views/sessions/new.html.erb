<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <div style="margin-left:-180px;">
    <%= label_tag :email %>
  </div>
  <%= text_field_tag :email %>
  <div style="margin-left:-160px;">
    <%= label_tag :password %>
  </div>
  <%= password_field_tag :password %>
  
  <p><%= submit_tag "Login", :class => "btn" %></p>
<% end %>

